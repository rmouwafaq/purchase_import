<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record model="ir.actions.act_window" id="create_facture_wizard_action">
            <field name="name">Add Factures</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">gestion_multi_magasins.create_facture_wizard</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
        <record model="ir.ui.view" id="new_attendee_wizard">
            <field name="name">New Factures</field>
            <field name="model">gestion_multi_magasins.create_facture_wizard</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Add Facture">
                    <group>
                        <field name="name"/>
                        <field name="account_id"/>
                        <field name="complementary_invoice_ids" widget="many2many"/>
                    </group>
                    <footer>
                      <button type="special" special="cancel" string="Cancel" icon="gtk-cancel"/>
                      <button type="object" name="action_add_factures" string="Calcul" icon="gtk-ok"/>
                    </footer>
                </form>
            </field>
          </record>
          <act_window id="import_create_facture_wizard" name="Add Factures" src_model="import.invoice"
                    res_model="gestion_multi_magasins.create_facture_wizard" view_mode="form" target="new"
                    key2="client_action_multi"/>
          <menuitem name="Add Facture" parent="gestion_multi_magasins_menu" id="create_facture_wizard_menu"
                  action="create_facture_wizard_action"/>          
        
    </data>
</openerp>   
    